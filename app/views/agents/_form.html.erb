<%= form_for(@agent, :html => {:class =>'form-stacked'}) do |f| %>
		
<%= error_messages_for(@agent) %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.label :email %>
  <%= f.text_field :email %>
	
	<%= f.label :phone %>
  <%= f.text_field :phone %>

  <%= f.label :license_number %>
  <%= f.text_field :license_number %>

  <%= f.label :broker_id %>
  <%= f.text_field :broker_id %><br />

  <%= f.submit :class => 'btn btn-success' %>

<% end %>
