<h1>Listing listings</h1>

<table>
  <tr>
    <th>Street1</th>
    <th>Street2</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Beds</th>
    <th>Baths</th>
    <th>Agent</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @listings.each do |listing| %>
  <tr>
    <td><%= listing.street1 %></td>
    <td><%= listing.street2 %></td>
    <td><%= listing.city %></td>
    <td><%= listing.state %></td>
    <td><%= listing.zip %></td>
    <td><%= listing.beds %></td>
    <td><%= listing.baths %></td>
    <td><%= listing.agent_id %></td>
    <td><%= link_to 'Show', listing %></td>
    <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
    <td><%= link_to 'Destroy', listing, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Listing', new_listing_path %>
