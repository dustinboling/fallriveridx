<%= form_for(@listing, :html => {:class => 'form'}) do |f| %>

<%= error_messages_for(@listing) %>

	<%= f.label :street1 %>
	<%= f.text_field :street1 %>

	<%= f.label :street2 %>
	<%= f.text_field :street2 %>

	<%= f.label :city %>
	<%= f.text_field :city %>

	<%= f.label :state %>
	<%= f.text_field :state %>

	<%= f.label :zip %>
	<%= f.number_field :zip %>

	<%= f.label :beds %>
	<%= f.number_field :beds %>

	<%= f.label :baths %>
	<%= f.number_field :baths %>

	<%= f.label :agent_id %>
	<%= f.number_field :agent_id %><br />

	<%= f.submit :class => 'btn btn-success' %>

<% end %>
