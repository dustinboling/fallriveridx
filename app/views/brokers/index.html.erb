<div class="container">
	<h1>Listing brokers</h1>

	<table class="table-striped span12">
	  <tr>
	    <th>Name</th>
	    <th>Street1</th>
	    <th>Street2</th>
	    <th>City</th>
	    <th>State</th>
	    <th>Zip</th>
	    <th>Phone</th>
			<th>License Number</th>
			<th></th>
	    <th></th>
	    <th></th>
	  </tr>

	<% @brokers.each do |broker| %>
	  <tr>
	    <td><%= broker.name %></td>
	    <td><%= broker.street1 %></td>
	    <td><%= broker.street2 %></td>
	    <td><%= broker.city %></td>
	    <td><%= broker.state %></td>
	    <td><%= broker.zip %></td>
			<td><%= broker.phone %></td>
			<td><%= broker.license_number %></td>
	    <td><%= link_to 'Show', broker %></td>
	    <td><%= link_to 'Edit', edit_broker_path(broker) %></td>
	    <td><%= link_to 'Destroy', broker, confirm: 'Are you sure?', method: :delete %></td>
	  </tr>
	<% end %>
	</table>

	<br />

	<%= link_to 'New Broker', new_broker_path %>
</div>